  "version": "0.2.0",
  "configurations": [
    

    {
      "name": "digicard",
      "request": "launch",
      "type": "dart",
      "args": [
        "-d", "chrome", 
        "--web-port", "8000"]
    },
    {
      "name": "digicard (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "args": [
        "-d", "chrome", 
        "--web-port", "8001"]
    },
    {
      "name": "digicard (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "args": [
        "-d", "chrome", 
        "--web-port", "8002"]
    }
  ]
}
